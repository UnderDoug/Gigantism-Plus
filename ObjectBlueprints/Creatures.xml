<?xml version="1.0" encoding="utf-8"?>
<objects>
    <!-- 
       Add NaturalEquipmentManager to Item
     -->
    <object Name="NaturalWeapon" Load="Merge">
      <part Name="NaturalEquipmentManager" />
    </object>
  
    <!-- 
        Make select creatures Gigantic: 
        - give mutation "GigantismPlus"
        - remove tag "Gigantic" 
        Give creatures InventoryGigantifier.
        Give creatures ObjectCreationAnalyzer.
     -->
    <object Name="Creature" Load="Merge">
      <part Name="InventoryGigantifier" />
      <!-- part Name="ObjectCreationAnalyzer" /-->
      <builder Name="Gigantified" ChanceOneIn="5000" />
      <skill Name="Tactics_Vault" />
    </object>
    <object Name="Templar" Load="Merge">
      <builder Name="Gigantified" ChanceOneIn="500" />
    </object>
    <object Name="Great Saltback" Load="Merge">
        <mutation Name="GigantismPlus" Level="3" />
        <removetag Name="Gigantic" />
        <tag Name="GigantismPlusColorChange" Value="false" />
    </object>
    <object Name="Chrome Pyramid" Load="Merge">
        <mutation Name="GigantismPlus" Level="7" />
        <removetag Name="Gigantic" />
