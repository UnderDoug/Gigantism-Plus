    <!-- 
        Putus Templar: Giant-Slayers
     -->
    <object Name="TemplarGiantSlayerBase" Inherits="Templar">
      <part Name="Render" DisplayName="Templar Giant-Slayer"
            RenderString="t" Tile="Creatures/GiantSlayer_Templar.png" 
            ColorString="&amp;Y" TileColor="&amp;r" DetailColor="Y"  
            />
      <part Name="Description" Short="An over-square jaw competes with =pronouns.possessive= broad, furrowed brow for posession of =pronouns.possessive= face. =pronouns.Possessive= posture hints at a once bent spine, and =pronouns.possessive= gait all but betrays it. =pronouns.Subjective= =verb:flex:afterpronoun= =pronouns.possessive= immense fists in subconcious anticipation of imminent glory, while =pronouns.possessive= deep-set, monochrome eyes flicker hopefully across the horizon." />
      <removeskill Name="LongBlades" />
      <removeskill Name="LongBladesProficiency" />
      <skill Name="Acrobatics" />
      <skill Name="Acrobatics_Jump" />
      <skill Name="Cudgel" />
      <skill Name="Cudgel_Expertise" />
      <tag Name="BaseObject" Value="*noinherit" />
      <tag Name="InventoryPopulationTable" Value="Templar Giant-Slayer Equipment Pool" />
      <builder Name="Gigantified" ChanceOneIn="1" />
    </object>

