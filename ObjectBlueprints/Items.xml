          Tile="items/sw_cloak_fancy.bmp" />
    <part Name="WrassleGear" RandomTiles="Items/Nanoweave_Cape_~01-07~.png" />
    <part Name="Armor" WornOn="Back" />
    <part Name="Physics" Weight="1" />
	</object>
	<object Name="WrassleGloves" Inherits="BaseWrassleGear">
