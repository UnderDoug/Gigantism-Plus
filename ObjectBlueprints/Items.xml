          Tile="Items/Nanoweave_Suit_01.png" />
    <part Name="WrassleGear" RandomTiles="Items/Nanoweave_Suit_~01-07~.png" />
    <part Name="Armor" WornOn="Body" />
    <part Name="Physics" Weight="3" />
	</object>
	<object Name="WrassleCape" Inherits="BaseWrassleGear">
