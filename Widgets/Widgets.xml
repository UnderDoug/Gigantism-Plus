<?xml version="1.0" encoding="utf-8" ?>
<objects>
  
  <!-- 
      
      Zone mutators, Tile mutators, Multi-tile presets, Quest triggers, UI widgets
      
   -->

  <object Name="RandomBoulderOrDebrisBase" Inherits="Widget">
    <part Name="Render" DisplayName="[RandomBoulderOrDebris]"
          RenderString="B" Tile="Widgets/RandomBoulderOrRubble.png"
          ColorString="&amp;y" DetailColor="K"/>
    <part Name="RandomBoulderOrDebris" />
  </object>
  
  <object Name="RandomBoulderOrDebrisLimestone" Inherits="RandomBoulderOrDebrisBase">
    <part Name="Render" DisplayName="[RandomBoulderOrDebrisLimestone]"
          ColorString="&amp;y" DetailColor="K"/>
    <part Name="RandomBoulderOrDebris" ColorLike="Limestone" Invert="True" />
  </object>
  
  <object Name="RandomBoulderOrDebrisGranite" Inherits="RandomBoulderOrDebrisBase">
    <part Name="Render" DisplayName="[RandomBoulderOrDebrisGranite]"
          ColorString="&amp;w" DetailColor="K"/>
    <part Name="RandomBoulderOrDebris" ColorLike="Granite" />
  </object>

