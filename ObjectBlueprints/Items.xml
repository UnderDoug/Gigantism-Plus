  <!-- 
  
      Modified BaseGame Natural Weapons 
      
  -->
  <object Name="Crystalline Point" Load="Merge">
		<part Name="Render" DisplayName="{{crystallized|crystalline}} {{crystallized|point}}" />
	</object>

  <!-- 
      
