    <!-- 
        Make select creatures Gigantic: 
        - give mutation "GigantismPlus"
        - remove tag "Gigantic" 
        Give creatures InventoryGigantifier.
     -->
    <object Name="Creature" Load="Merge">
      <part Name="InventoryGigantifier" />
      <builder Name="Gigantified" ChanceOneIn="5000" />
    </object>
